<!-- A DrawerLayout is intended to be used as the top-level content view using match_parent for both width and height to consume the full space available. -->
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--tools:context=".Home">-->

    <!-- As the main content view, the view below consumes the entire
         space available using match_parent in both dimensions. -->
    <FrameLayout android:id="@+id/container" android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </FrameLayout>

    <!-- android:layout_gravity="start" tells DrawerLayout to treat
         this as a sliding drawer on the left side for left-to-right
         languages and on the right side for right-to-left languages.
         If you're not building against API 17 or higher, use
         android:layout_gravity="left" instead. -->
    <!-- The drawer is given a fixed width in dp and extends the full height of
         the container. -->
    <fragment android:id="@+id/navigation_drawer"
        android:layout_width="@dimen/navigation_drawer_width" android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.NavigationDrawerFragment"
        tools:layout="@layout/fragment_navigation_drawer" />

    <fragment android:id="@+id/permohonan_doa"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.PermohonanDoaFragment"
        tools:layout="@layout/fragment_permohonan_doa" />

    <fragment android:id="@+id/jadwal_pelayanan"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.JadwalPelayananFragment"
        tools:layout="@layout/fragment_permohonan_doa" />

    <fragment android:id="@+id/alkitab"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.AlkitabFragment"
        tools:layout="@layout/fragment_alkitab" />

    <fragment android:id="@+id/event"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.EventFragment"
        tools:layout="@layout/fragment_event" />

    <fragment android:id="@+id/renunganGema"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.RenunganGemaFragment"
        tools:layout="@layout/fragment_renungan_gema" />

    <fragment android:id="@+id/preMaritalClass"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.PreMaritalClassFragment"
        tools:layout="@layout/fragment_pre_marital_class" />

    <fragment android:id="@+id/katekisasi"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.KatekisasiFragment"
        tools:layout="@layout/fragment_katekisasi" />

    <fragment android:id="@+id/kelompokKecil"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.KelompokKecilFragment"
        tools:layout="@layout/fragment_kelompok_kecil" />

    <fragment android:id="@+id/tentangKami"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.TentangKamiFragment"
        tools:layout="@layout/fragment_tentang_kami" />

    <fragment android:id="@+id/hubungiKami"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:name="fingertech.mobileclientgky.HubungiKamiFragment"
        tools:layout="@layout/fragment_hubungi_kami" />

    <![CDATA[-->]]>

</android.support.v4.widget.DrawerLayout>